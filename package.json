{
  "name": "autorizador-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --config ./test/jest-e2e.json --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/bull": "0.0.2",
    "@nestjs/common": "7.0.8",
    "@nestjs/core": "7.0.8",
    "@nestjs/jwt": "^6.1.2",
    "@nestjs/mongoose": "^6.4.0",
    "@nestjs/passport": "^6.2.0",
    "@nestjs/platform-express": "^6.11.11",
    "@nestjs/schedule": "^0.2.1",
    "@nestjs/swagger": "^4.8.0",
    "@rimiti/hl7-object-parser": "^2.9.0",
    "bcrypt": "^4.0.1",
    "bull": "^3.13.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "express-list-endpoints": "^4.0.1",
    "express-list-routes": "^0.1.4",
    "flat": "^5.0.2",
    "helmet": "^3.22.0",
    "mongoose": "^5.12.13",
    "mongoose-auto-increment": "^5.0.1",
    "mongoose-auto-increment-reference": "^5.0.11",
    "mongoose-autopopulate": "^0.11.0",
    "mongoose-field-encryption": "^3.0.4",
    "mongoose-sequence": "^5.2.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "swagger-ui-express": "^4.1.4",
    "xml2json": "^0.12.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.2",
    "@nestjs/schematics": "^6.9.4",
    "@nestjs/testing": "^6.11.11",
    "@types/bull": "^3.12.2",
    "@types/express": "^4.17.6",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.12.36",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
