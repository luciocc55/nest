{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "moduleDirectories": [
    "node_modules",
    "/src"
  ],
  "roots": [
    "<rootDir>",
    "/src"
  ],
  "modulePaths": [
    "<rootDir>",
    "/src"
  ],
  "testEnvironment": "node",
  "testRegex": ".spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
